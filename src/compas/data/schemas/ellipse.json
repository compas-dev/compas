{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ellipse.json",
    "$compas": "1.7.1",
    "type": "object",
    "properties": {
        "plane": {"$ref": "compas.json#/definitions/plane"},
        "major": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "minor": {
            "type": "number",
            "exclusiveMinimum": 0
        }
    },
    "required": [
        "plane",
        "major",
        "minor"
    ]
}
